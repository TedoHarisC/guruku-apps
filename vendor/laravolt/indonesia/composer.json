{
    "name": "laravolt/indonesia",
    "description": "Package Laravel yang berisi data Provinsi, Kabupaten/Kota, Kecamatan, dan Keluarahan/Desa di seluruh Indonesia.",
    "keywords": ["laravel", "laravolt", "indonesia", "provinsi", "kabupaten", "kota", "kecamatan", "kelurahan", "desa"],
    "license": "MIT",
    "authors": [
        {
            "name": "Akbar Adhatama",
            "email": "am.adhatama@gmail.com"
        },
        {
            "name": "Deri Ramdani",
            "email": "deri.ramdani1@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravolt\\Indonesia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Indonesia\\Test\\": "tests/"
        }
    },
    "require": {
        "php" : ">=5.5.0",
        "illuminate/support": "~5.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "squizlabs/php_codesniffer": "2.*",
        "orchestra/testbench": "~3.0"
    },
   "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
       "laravel": {
           "providers": [
               "Laravolt\\Indonesia\\ServiceProvider"
           ],
           "aliases": {
               "Indonesia" : "Laravolt\\Indonesia\\Facade"
           }
       }
   }
}
